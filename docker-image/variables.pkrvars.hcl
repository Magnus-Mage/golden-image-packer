# OpenStack-specific variables
# Update these values based on your OpenStack environment

# Get this from: openstack image list
source_image = "ubuntu-22.04"

# Get this from: openstack flavor list
flavor = "m1.small"

# Get this from: openstack network list
# Replace with your actual network UUID
network = "your-network-uuid-here"

# Customize your image name
image_name = "ubuntu-dev-v1.0"
